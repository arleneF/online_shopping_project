extends layout

block content
	form(method = 'POST' action = '/login')
		div.form-group
			lable(for = 'email') Email:
		div.form-group
			input#name.form-control(type = 'email', placeholder = 'name@email.com' name = 'email')
		div.form-group
			lable(for = 'pw') Password:
		div.form-group
			input#pw.form-control(type = 'password' name = 'password')
		button.btn.btn-primary(type='submit') Login